{
  "name": "create-react-app-with-styled-components-typescript",
  "version": "5.0.0",
  "private": true,
  "dependencies": {
    "@mui/lab": "latest",
    "@mui/material": "latest",
    "@mui/styled-engine-sc": "latest",
    "@testing-library/jest-dom": "latest",
    "@testing-library/react": "latest",
    "@testing-library/user-event": "latest",
    "i18next": "^21.6.6",
    "i18next-browser-languagedetector": "^6.1.2",
    "react": "latest",
    "react-dom": "latest",
    "react-i18next": "^11.15.3",
    "react-redux": "^7.2.6",
